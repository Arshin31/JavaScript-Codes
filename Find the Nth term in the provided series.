// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 

  var N=userInput;
  
  if(N%2===0)
  {
      N= N*N;
  }
  else if(N<0 && N===0)
  {
      console.log("Error");
  }
  else
  {
      N= N*N*N;
  }
  console.log(N);

  //end-here
});
